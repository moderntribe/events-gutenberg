{
    "verbose": true,
    "testURL": "http://localhost/",
    "globals": {
        "NODE_ENV": "test"
    },
    "setupFiles": [
        "<rootDir>/jest.setup.js"
    ],
    "projects": [
        {
            "displayName": "common",
            "testMatch": ["<rootDir>/plugins/common/*"]
        },
        {
            "displayName": "events",
            "testMatch": ["<rootDir>/plugins/events/*"]
        }
    ],
    "moduleNameMapper": {
    },
    "testEnvironment": "jest-environment-jsdom-global",
    "testPathIgnorePatterns": [
        "/node_modules/",
        "/lang/",
        "/i18n/",
        "/src/views",
        "/src/Tribe",
        "/src/resources"
    ]
}
