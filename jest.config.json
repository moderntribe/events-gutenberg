{
  "verbose": true,
  "testURL": "http://localhost/",
  "globals": {
    "NODE_ENV": "test"
  },
  "projects": [
    {
      "displayName": "common",
      "setupFiles": [
        "<rootDir>/jest.setup.js"
      ],
      "testEnvironment": "jest-environment-jsdom-global",
      "testMatch": [
        "<rootDir>/plugins/common/**/__tests__/**/*.js?(x)"
      ],
      "testURL": "http://localhost/"
    },
    {
      "displayName": "events",
      "setupFiles": [
        "<rootDir>/jest.setup.js"
      ],
      "testEnvironment": "jest-environment-jsdom-global",
      "testMatch": [
        "<rootDir>/plugins/events/**/__tests__/**/*.js?(x)"
      ],
      "testURL": "http://localhost/",
      "moduleNameMapper": {
        "^.+\\.(p?css)$": "<rootDir>/plugins/events/__mocks__/style.js",
        "\\.(svg)$": "<rootDir>/plugins/events/__mocks__/icons.js"
      }
    }
  ],
  "testEnvironment": "jest-environment-jsdom-global",
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/lang/",
    "/i18n/",
    "/src/views",
    "/src/Tribe",
    "/src/resources"
  ]
}
