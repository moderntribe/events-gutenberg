{
	"name": "events-gutenberg",
	"version": "0.2.5-alpha",
	"description": "A new WordPress editor experience",
	"repository": "git+https://github.com/WordPress/gutenberg.git",
	"author": "Modern Tribe",
	"license": "GPL-2.0-or-later",
	"private": true,
	"keywords": [
		"WordPress",
		"editor",
		"The Events Calendar"
	],
	"engines": {
		"node": ">=8.0.0",
		"npm": ">=5.0.0"
	},
	"scripts": {
		"build": "cross-env BABEL_ENV=default NODE_ENV=production webpack",
		"gettext-strings": "cross-env BABEL_ENV=gettext webpack",
		"dev": "cross-env BABEL_ENV=default webpack --watch",
		"test": "TZ=UTC jest",
		"test:travis": "npm run test -- --maxWorkers=4",
		"lint": "eslint --quiet --parser=babel-eslint 'src/modules/**/*.js'",
		"lint:fix": "eslint --fix --quiet --parser=babel-eslint 'src/modules/**/*.js'"
	},
	"dependencies": {
		"@wordpress/a11y": "1.0.3",
		"@wordpress/data": "^1.1.0",
		"@wordpress/i18n": "^1.2.2",
		"@wordpress/is-shallow-equal": "^1.1.2",
		"@wordpress/url": "1.0.0",
		"chrono-node": "^1.3.5",
		"classnames": "^2.2.6",
		"file-loader": "^1.1.6",
		"google-maps-react": "^1.1.11",
		"he": "^1.1.1",
		"jest": "^23.3",
		"jquery": "3.2.1",
		"lodash": "^4.17.10",
		"moment": "2.19.3",
		"moment-timezone": "0.5.13",
		"prop-types": "^15.6.2",
		"querystringify": "^1.0.0",
		"react": "16.3.0",
		"react-day-picker": "^7.1.10",
		"react-dom": "16.3.0",
		"react-google-maps": "^9.4.5",
		"react-input-autosize": "^2.2.1",
		"react-places-autocomplete": "^6.1.2",
		"react-redux": "^5.0.7",
		"react-scroll-to": "^1.2.2",
		"react-text-input": "^0.0.8",
		"redux": "^4.0.0",
		"redux-thunk": "^2.3.0",
		"reselect": "^3.0.1",
		"url-loader": "^0.6.2",
		"uuid": "^3.3.2",
		"validator": "^9.4.1"
	},
	"devDependencies": {
		"@babel/runtime": "7.0.0-beta.52",
		"@wordpress/hooks": "^1.3.2",
		"autoprefixer": "6.7.7",
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.2.6",
		"babel-jest": "^23.4.0",
		"babel-loader": "7.1.2",
		"babel-plugin-import-glob": "2.0.0",
		"babel-plugin-lodash": "3.2.11",
		"babel-plugin-syntax-class-properties": "^6.13.0",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-object-rest-spread": "6.26.0",
		"babel-plugin-transform-react-jsx": "6.24.1",
		"babel-plugin-transform-runtime": "6.23.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babel-traverse": "6.26.0",
		"cross-env": "^5.2.0",
		"css-loader": "0.28.9",
		"css-mqpacker": "^4.0.1",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"eslint": "^4.19.1",
		"eslint-config-products": "github:moderntribe/eslint-config-products#v0.1.1",
		"eslint-plugin-react": "^7.10.0",
		"extract-text-webpack-plugin": "^3.0.2",
		"gettext-parser": "1.3.0",
		"jest-environment-jsdom": "^23.4.0",
		"jest-environment-jsdom-global": "^1.1.0",
		"node-sass": "^4.9.2",
		"postcss-cssnext": "^2.5.2",
		"postcss-hexrgba": "^1.0.0",
		"postcss-import": "^11.0.0",
		"postcss-loader": "2.0.6",
		"postcss-mixins": "^4.0.1",
		"postcss-nested": "^1.0.0",
		"react-svg-loader": "^2.1.0",
		"react-test-renderer": "^16.4.1",
		"redux-devtools-extension": "^2.13.5",
		"redux-mock-store": "^1.5.3",
		"regenerator-runtime": "^0.11.1",
		"sass-variables-loader": "0.1.3",
		"snapshot-diff": "^0.4.0",
		"style-loader": "0.18.2",
		"webpack": "3.10.0"
	},
	"jest": {
		"verbose": true,
		"testURL": "http://localhost/",
		"globals": {
			"NODE_ENV": "test"
		},
		"setupFiles": [
			"<rootDir>/jest.setup.js"
		],
		"moduleNameMapper": {
			"^utils(.*)$": "<rootDir>/src/modules/editor/utils$1",
			"^editor(.*)$": "<rootDir>/src/modules/editor$1",
			"^data(.*)$": "<rootDir>/src/modules/data$1",
			"^elements(.*)$": "<rootDir>/src/modules/elements$1",
			"\\.(css|pcss)$": "<rootDir>/__mocks__/style.js",
			"\\.(svg)$": "<rootDir>/__mocks__/icons.js"
		},
		"testEnvironment": "jest-environment-jsdom-global",
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/lang/",
			"/i18n/",
			"/src/views",
			"/src/Tribe",
			"/src/resources"
		]
	}
}
