.tribe-editor__container-panel {
	border: 1px solid #E1E3E6;
	display: flex;
}

.tribe-editor__container-panel__icon {
	flex: none;
	width: 100px;
	padding: 25px 20px 20px;
}

.tribe-editor__container-panel__header-content-wrapper {
	background-color: #F7F8F9;
	border-left: 1px dashed #B5BCC2;
	flex: auto;
}

.tribe-editor__container-panel__header {
	padding: 25px;
}

.tribe-editor__container-panel__content {
	margin: 0 25px;
	padding: 30px 0;
}
