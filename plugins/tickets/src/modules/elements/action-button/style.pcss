.tribe-editor__tickets-btn--action {
	display: flex;
	align-items: center;
	font-family: Helvetica, sans-serif, arial;
	font-size: 15px;
	line-height: 18px;
	letter-spacing: 0.38px;
	color: #545D66;
	cursor: pointer;

	svg {
		fill: #545D66;

		path {
			fill: #545D66;
		}
	}
}

.tribe-editor__tickets-btn--action-right {
	svg {
		margin-left: 6px;
		order: 2;
	}
}

.tribe-editor__tickets-btn--action-left {
	svg {
		margin-right: 6px;
	}
}
