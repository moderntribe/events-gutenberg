.tribe-editor__label-with-tooltip {
	display: flex;
	align-items: center;
}

.tribe-editor__labeled-item__label {

	.tribe-editor__label-with-tooltip & {
		flex: none;
	}
}

.tribe-editor__label-with-tooltip__tooltip-label {
	flex: none;
	background-color: transparent;
	border: none;
	padding: 0;
	display: inline-flex;
	align-items: center;

	& > * {
		flex: none;
	}
}
