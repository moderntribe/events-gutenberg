.tribe-editor__tickets {
	font-family: Helvetica, sans-serif, arial;
	padding-bottom: 40px;

	&--selected {
		padding-bottom: 0;
	}

	.block-list-appender {
		display: none;
	}
}

.tribe-editor__tickets--loading {
	display: flex;
	align-items: center;
	justify-content: center;
	padding: 20px;

	.components-spinner {
		flex: none;
	}
}
