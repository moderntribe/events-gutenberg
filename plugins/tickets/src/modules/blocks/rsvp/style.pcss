.tribe-editor__rsvp {
	font-family: Helvetica, sans-serif, arial;
}

.tribe-editor__inactive-block__icon {
	display: flex;
	flex-direction: column;
	align-items: center;

	& > svg {
		flex: none;
	}
}

.tribe-editor__rsvp__inactive-block-icon-label {
	color: #AEB4BB;
	flex: none;
	font-weight: bold;
	font-size: 16px;
	line-height: 19px;
	letter-spacing: 0.05px;
}
