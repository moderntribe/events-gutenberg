/* increased specificity required to override gutenberg editor styles */
input[type].tribe-editor__input {
	color: #000000;
	font-size: 16px;
	line-height: 20px;
	border: 1px solid #E1E3E6;
	box-shadow: inset 0 1px 0 0 #F1F1F1;
	padding: 9px 15px;
	height: 40px;

	&:disabled {
		color: #AEB4BB;
	}
}
