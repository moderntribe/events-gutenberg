.tribe-editor__from-time-range-picker__content {
	display: flex;
	align-items: center;

	& > span {
		font-size: 16px;
		line-height: 24px;
		color: #8D949B;
	}

	& > * {
		flex: none;
		margin-right: 15px;

		&:last-child {
			margin-right: 0;
		}
	}
}

.tribe-editor__from-time-range-picker__multi-day-checkbox {
	margin-left: 5px;
}
